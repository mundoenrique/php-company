	#	VIRTUALHOST HTTPS
SetEnv BASE_URL https://empresas.novopayment.lc/
SetEnv ASSET_URL https://empresas.novopayment.lc/assets/
SetEnv COOKIE_DOMAIN empresas.novopayment.lc
SetEnv COOKIE_SECURE ON

SetEnv ASSET_PATH {{ubicación-física}}/site-conexionempresas/build/assets/
SetEnv LOG_PATH ''
SetEnv ENCRYPTION_KEY n0v0p4ym3nt-k3y
# SetEnv SESS_DRIVER database
# SetEnv SESS_SAVE_PATH ceo_sessions
SetEnv SESS_DRIVER files
SetEnv SESS_SAVE_PATH {{ubicación-física}}/sessions/ceo
SetEnv SESS_COOKIE_NAME empresas
SetEnv SESS_EXPIRATION 1800
SetEnv COOKIE_PREFIX 'ceo_'
SetEnv PROXY_ENABLE OFF

#----------------------#
# APPLICATION SETTINGS #
#----------------------#
SetEnv ACTIVE_SAFETY ON
SetEnv CYPHER_BASE 0123456789=abcdefghijklmnopqrstuvwxyz?ABCDEFGHIJKLMNOPQRSTUVWXYZ=
SetEnv ACCESS_URL 'pe, bdb, bnt, bp, co, us, ve, pb, bg, vg, coop, bog, bpi, per, usd, ven, col'
SetEnv ACTIVE_RECAPTCHA ON
SetEnv IP_VERIFY OFF
SetEnv DB_VERIFY ON
SetEnv SINGLE_SIGN_ON OFF
SetEnv API_CONTENT_URL 172.24.6.123:3001/
SetEnv UPLOAD_PATH {{ubicación-física}}/upload/
SetEnv API_GEE_WAY OFF

